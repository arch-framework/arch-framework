{
    "extends": ["../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.json"],
            "parser": "jsonc-eslint-parser",
            "rules": {
                "@nx/dependency-checks": "error"
            }
        },
        {
            "files": ["./package.json", "./generators.json"],
            "parser": "jsonc-eslint-parser",
            "rules": {
                "@nx/nx-plugin-checks": "error"
            }
        }
    ]
}
