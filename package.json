    "scripts": {
        "nx": "nx",
        "dependency:reset": "rm -rf node_modules package-lock.json && npm i",
        "postinstall": "husky install",
        "postversion": "npm run build:libs:prod",
        "website:server:start": "nx start website",
        "website:build": "nx build website",
        "website:deploy": "nx deploy website",
        "example:server:ssr": "nx serve-ssr example --configuration development",
        "example:server:start": "nx serve example --configuration development",
        "lint": "nx run-many -t lint --parallel --maxParallel 3",
        "lint:fix": "npm run lint -- --fix",
        "format": "nx format:write",
        "publish:libs": "lerna publish --no-private --force-publish",
        "build:libs:prod": "nx run-many --target build --projects=tag:scope:public --prod --skip-nx-cache"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,html,md,less,scss,json,yml}": [
            "nx format:write"
